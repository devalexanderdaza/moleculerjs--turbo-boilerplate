# .env.example
# Environment
NODE_ENV=development

# Service information
SERVICE_NAME=sample-service
SERVICE_VERSION=1.0.0

# API configuration
API_PORT=3000
CORS_ORIGINS=*

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=microservice
DB_USER=postgres
DB_PASSWORD=postgres

# Redis configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Moleculer configuration
MOLECULER_NAMESPACE=microservices
MOLECULER_TRANSPORTER=redis://localhost:6379
MOLECULER_LOG_LEVEL=info
MOLECULER_METRICS=false
MOLECULER_TRACING=false

# Logging
LOG_LEVEL=info